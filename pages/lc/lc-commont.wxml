
<!-- 流程列表 -->
<template name="lc-commont-template">
<view hover-class="bc-select" class="bc-unchecked"  bind:touchstart="itemTouchStart" bind:touchend="itemTouchEnd" bind:tap="itemTap">

<view class="lc-list-bh">{{item.objno}}</view>
<view class="lc-list-title">{{item.objname}}</view>

<view class="lc-list-item">
<text class="lc-list-item-author">创建：{{item.creatorName}}</text>
<text class="lc-list-item-time">创建时间：{{item.createdate}}</text>
</view>
</view>
</template>


