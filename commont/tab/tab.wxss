
.nav {
  width: 100%;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  z-index:2;
}

/* 未选中样式 */
.unchecked {
  line-height: 100rpx;
  text-align: center;
  flex: 1;
  color: #000;
  font-size: 16px;

}

/* 选中样式 */
.select {
  line-height: 100rpx;
  text-align: center;
  color: #387ef5;
  flex: 1;
  font-size: 16px;
  border-bottom: 2px solid #387ef5;
}

/* 内容显示样式*/
.show {
  display: block;

}

/* 内容隐藏样式 */
.hidden {
  display: none;

}
